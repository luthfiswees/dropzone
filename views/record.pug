html
  head
    //- import interactjs and bootstrap
    link(rel='stylesheet', href='/stylesheets/bootstrap/css/bootstrap.css')
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js")
    
    //- import Highcharts(for chart display) and data.js where we put it 
    script(src="https://code.highcharts.com/highcharts.js")
    script(src="/javascripts/record.js")
    
  body
    include navbar.pug
    br
    br
    
    h3 Overall Activity Statistics
    div(id="line_container", style="width:100%; height:300px;")
    div(class="container")
      div(class="row")
        div(class="col-md-6")
          div(id="pie_container")
        div(class="col-md-6")
          div(id="pie_container_undefined")
    br 
    br
      
    h3 Latest Activity Statistics
    div(id="latest_game_line_container", style="width:100%; height:300px;")
    div(class="container")
      div(class="row")
        div(class="col-md-6")
          div(id="latest_game_pie_container")
        div(class="col-md-6")
          div(id="latest_game_pie_container_undefined")
    br 
    br 
    br
    
    
    
